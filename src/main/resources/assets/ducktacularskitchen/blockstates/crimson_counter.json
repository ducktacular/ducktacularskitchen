{
  "multipart": [
    {
      "apply": {
        "model": "ducktacularskitchen:block/crimson_counter_top",
        "uvlock": true
      }
    },
    {
      "apply": {
        "model": "ducktacularskitchen:block/crimson_counter"
      }
    },
    {
      "when": {
        "north": true
      },
      "apply": {
        "model": "ducktacularskitchen:block/crimson_counter_side"
      }
    },
    {
      "when": {
        "east": true
      },
      "apply": {
        "model": "ducktacularskitchen:block/crimson_counter_side",
        "y": 90
      }
    },
    {
      "when": {
        "south": true
      },
      "apply": {
        "model": "ducktacularskitchen:block/crimson_counter_side",
        "y": 180
      }
    },
    {
      "when": {
        "west": true
      },
      "apply": {
        "model": "ducktacularskitchen:block/crimson_counter_side",
        "y": 270
      }
    }
  ]
}
